{
  "name": "ziv-cocktails-dashboard",
  "private": true,
  "version": "0.0.1",
  "type": "commonjs",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "concurrently -k -n VITE,TS,ELECTRON -c cyan,magenta,green \"vite\" \"tsc -p electron -w\" \"wait-on file:dist-electron/main.js && electronmon .\"",
    "build": "tsc -p electron && vite build",
    "preview": "vite preview",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "echo 'lint not configured'"
  },
  "dependencies": {
    "firebase": "^10.14.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "electron": "^32.2.0",
    "electronmon": "^2.0.3",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3",
    "vite": "^5.4.11",
    "wait-on": "^8.0.1"
  }
}
