{
  "name": "ziv-cocktails-dashboard",
  "private": true,
  "version": "1.0.0",
  "type": "commonjs",
  "author": "Offir Tura",
  "main": "dist/electron/main.js",
  "scripts": {
    "dev": "concurrently -k -n VITE,TS,ELECTRON -c cyan,magenta,green \"vite\" \"tsc -p electron -w\" \"wait-on file:dist/electron/main.js && electronmon .\"",
    "build": "tsc -p electron && vite build",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "preview": "vite preview",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "make:icons": "node src/ui/make-icons.js",
    "f1:build": "tsc -p backend/tsconfig.json",
    "f1:run:mock": "node dist/engine/run/run-mock.js",
    "f1:run:live": "node dist/engine/run/run-live.js",
    "f1:run:mock:build": "npm run f1:build && npm run f1:run:mock",
    "f1:run:live:build": "npm run f1:build && npm run f1:run:live",
    "f1:assert": "node dist/engine/run/assert-mock-output.js",
    "f1:assert:build": "npm run f1:build && npm run f1:assert",
    "f1:assert:update": "node dist/engine/run/assert-mock-output.js --update",
    "f1:assert:update:build": "npm run f1:build && npm run f1:assert:update",
    "f2:run:mock": "node dist/engine/run/run-snapshots-mock.js",
    "f2:run:mock:build": "npm run f1:build && npm run f2:run:mock",
    "f2:assert": "node dist/engine/run/assert-snapshots-mock.js",
    "f2:assert:build": "npm run f1:build && npm run f2:assert",
    "f2:assert:update": "node dist/engine/run/assert-snapshots-mock.js --update",
    "f2:assert:update:build": "npm run f1:build && npm run f2:assert:update",
    "lint": "echo 'lint not configured'"
  },
  "dependencies": {
    "firebase": "^12.7.0",
    "jspdf": "^3.0.4",
    "lucide-react": "^0.562.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "electronmon": "^2.0.3",
    "png-to-ico": "^2.1.8",
    "postcss": "^8.4.49",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3",
    "vite": "^7.3.0",
    "wait-on": "^8.0.1"
  }
}
